<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tallygo.tallygoexamples">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".report_server.ReportCurrentLocServerActivity"/>
        <activity android:name=".report_server.ReportEtaServerActivity"/>
        <activity android:name=".report_server.ReportRouteServerActivity"/>

        <activity android:name=".display_driver_report.DisplayDriverReportActivity"/>


        <meta-data android:name="tallygo.ACCESS_TOKEN" android:value="@string/tallygo_access_token" />

    </application>

</manifest>